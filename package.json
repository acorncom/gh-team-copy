{
  "name": "gh-team-copy",
  "version": "1.0.0",
  "description": "Copy teams and members from an existing org to a new one",
  "main": "build/index.js",
  "config": {
    "MOCHA_OPTS": "--compilers js:babel-register --timeout 5000"
  },
  "scripts": {
    "build": "rm -rf build/*; babel src -d build",
    "start": "npm run build && node build/index.js",
    "test": "mocha $npm_package_config_MOCHA_OPTS tests/*.js"
  },
  "author": "Sivakumar Kailasam <sivakumar.ur.friend@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-register": "6.8.0",
    "github": "0.2.4",
    "mocha": "2.4.5",
    "mocha-only-detector": "0.1.0"
  }
}
